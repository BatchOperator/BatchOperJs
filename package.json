{
  "name": "libsugar",
  "version": "2.12.0-beta.2",
  "description": "",
  "main": "./cjs/index.js",
  "module": "./esm/index.mjs",
  "browser": "./dist/sugar.min.js",
  "umd::main": "./dist/sugar.js",
  "types": "./type/index.d.ts",
  "typings": "./type/index.d.ts",
  "files": [
    "/**/*.d.ts",
    "src/**/*.ts",
    "type/**/*.d.ts",
    "cjs/**/*.js",
    "cjs/**/*.ts",
    "dist/**/*.js",
    "es/**/*.js",
    "es/**/*.ts",
    "esm/**/*.mjs",
    "esm/**/*.ts",
    "umd/**/*.js",
    "umd/**/*.ts",
    "sugar.js",
    "sugar.mjs"
  ],
  "exports": {
    ".": {
      "import": "./esm/index.mjs",
      "require": "./cjs/index.js"
    },
    "./*": {
      "import": "./esm/*.mjs",
      "require": "./cjs/*.js"
    },
    "./bop/*": {
      "import": "./esm/bop/*.mjs",
      "require": "./cjs/bop/*.js"
    },
    "./math/*": {
      "import": "./esm/math/*.mjs",
      "require": "./cjs/math/*.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "tsc": "tsc",
    "rollup": "rollup",
    "babel": "babel",
    "prebuild": "node ./dev/prebuild.mjs",
    "build": "darl build",
    "postbuild": "node ./dev/postbuild.mjs",
    "test": "jest --coverage",
    "doc": "typedoc --out docs src --exclude **/*.test.ts --name \"Sugar.js\"",
    "postdoc": "node ./dev/postdoc.mjs",
    "do:pack": "node ./dev/publish.mjs pack",
    "do:publish": "node ./dev/publish.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/libsugar/sugar.js.git"
  },
  "keywords": [
    "sugar",
    "batchoper"
  ],
  "author": "libsugar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/libsugar/sugar.js/issues"
  },
  "homepage": "https://github.com/libsugar/sugar.js#readme",
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-modules-umd": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/jest": "^26.0.19",
    "babel-plugin-add-import-extension": "^1.5.1",
    "darl": "^2.1.1",
    "jest": "^26.6.3",
    "ncp": "^2.0.0",
    "rollup": "^2.35.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typedoc": "^0.21.2",
    "typescript": "^4.1.3"
  },
  "optionalDependencies": {
    "tslib": "^2.0.3"
  }
}
