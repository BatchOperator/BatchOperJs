{
    "name": "@libsugar/seq",
    "version": "3.0.0",
    "description": "Like syntactic sugar, but is library",
    "author": "libsugar",
    "homepage": "https://github.com/libsugar/sugar.js#readme",
    "keywords": [
        "sugar",
        "batchoper"
    ],
    "license": "MIT",
    "main": "cjs/index.js",
    "module": "esm/index.mjs",
    "types": "type",
    "files": [
        "/**/*.d.ts",
        "src/**/*.ts",
        "es/**/*.js",
        "esm/**/*.mjs",
        "cjs/**/*.js",
        "umd/**/*.js",
        "dist/**/*.js",
        "dist/**/*.mjs",
        "dist/**/*.cjs"
    ],
    "exports": {
        ".": {
            "import": "./esm/index.mjs",
            "require": "./cjs/index.js"
        },
        "./*": {
            "import": "./esm/*.mjs",
            "require": "./cjs/*.js"
        },
        "./package.json": "./package.json"
    },
    "scripts": {
        "swc": "swc",
        "tsc": "tsc",
        "babel": "babel",
        "rollup": "rollup",
        "build": "darl build",
        "test": "jest --coverage"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/libsugar/sugar.js.git"
    },
    "bugs": {
        "url": "https://github.com/libsugar/sugar.js/issues"
    },
    "dependencies": {
        "@libsugar/adt": "^3.0.0",
        "@libsugar/box": "^3.0.0",
        "@libsugar/fn": "^3.0.0",
        "@libsugar/iter": "^3.0.0",
        "@libsugar/types": "^3.0.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.17.10",
        "@babel/core": "^7.17.10",
        "@babel/plugin-transform-runtime": "^7.17.10",
        "@babel/preset-env": "^7.17.10",
        "@rollup/plugin-typescript": "^8.3.2",
        "@swc/cli": "^0.1.57",
        "@swc/core": "^1.2.182",
        "@swc/jest": "^0.2.21",
        "babel-plugin-add-import-extension": "^1.6.0",
        "darl": "^2.2.0",
        "jest": "^28.1.0",
        "rollup": "^2.72.1",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-terser": "^7.0.2",
        "ts-node": "^10.7.0",
        "typedoc": "^0.22.15",
        "typescript": "^4.6.4",
        "ts-jest": "^28.0.2"
    }
}
